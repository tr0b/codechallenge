@model IEnumerable<codechallenge.Models.tbUser>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PK_UserID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.First_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Last_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PK_UserID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.First_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Last_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PK_UserID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PK_UserID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PK_UserID })
        </td>
    </tr>
}

</table>
